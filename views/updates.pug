doctype html
html 
    head 
        title= `${title}`
        meta(name="viewpet" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        .content
            h1.index Update Custom Object Form | Integrating With HubSpot I Practicum.
            a(href="/update-cobj") Create/Update 
            .cards 
                each Sport in data
                    .card 
                        h2.card__name #{Sport.properties.id}
                        p.card__country Nombre: #{Sport.properties.nombre}
                        p.card__outstanding_athlete Edad: #{Sport.properties.edad}